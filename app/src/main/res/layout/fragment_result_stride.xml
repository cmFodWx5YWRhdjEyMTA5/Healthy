<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:dashboardview="http://schemas.android.com/apk/res-auto"
    xmlns:HeightCurve="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation="vertical"
        android:background="#FFF"
        android:layout_marginBottom="@dimen/x50">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x128"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:background="#FFF">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="对称性"
                android:textColor="#01bf9d"
                android:textSize="@dimen/x52"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="了解对称性"
                android:textColor="#808080"
                android:textSize="@dimen/x36"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/y848"
            android:gravity="center"
            android:background="@drawable/bg_lineargradient_threecolor">
            <com.amsu.healthy.view.CircleRingView
                android:id="@+id/vr_strid_symmetry"
                android:layout_width="@dimen/x600"
                android:layout_height="@dimen/x600"
                dashboardview:circlering_width="@dimen/x76"
                dashboardview:circlering_background_color="#BBB"
                dashboardview:circlering_start_color="#01bf9d"
                dashboardview:circlering_end_color="#01bf9d"
                android:layout_centerInParent="true"/>

            <RelativeLayout
                android:layout_width="@dimen/x452"
                android:layout_height="@dimen/x600"
                android:layout_centerInParent="true">
                <TextView
                    android:id="@+id/tv_home_smmetry_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textSize="@dimen/x128"
                    android:textColor="#545454"
                    android:layout_centerInParent="true"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/tv_home_smmetry_value"
                    android:text="对称性"
                    android:textSize="@dimen/x36"
                    android:textColor="#545454"
                    android:layout_marginBottom="@dimen/y10"
                    android:layout_centerHorizontal="true"/>
                <TextView
                    android:id="@+id/tv_home_smmetry_dec"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_home_smmetry_value"
                    android:text="--"
                    android:textSize="@dimen/x36"
                    android:textColor="#01bf9d"
                    android:layout_marginTop="@dimen/y10"
                    android:layout_centerHorizontal="true"/>
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x128"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:background="#FFF">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="一致性"
                android:textColor="#01bf9d"
                android:textSize="@dimen/x52"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="了解一致性"
                android:textColor="#808080"
                android:textSize="@dimen/x36"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/y848"
            android:gravity="center"
            android:background="@drawable/bg_lineargradient_threecolor">
            <com.amsu.healthy.view.CircleRingView
                android:id="@+id/vr_strid_variability"
                android:layout_width="@dimen/x600"
                android:layout_height="@dimen/x600"
                dashboardview:circlering_width="@dimen/x76"
                dashboardview:circlering_background_color="#BBB"
                dashboardview:circlering_start_color="#1fcf6d"
                dashboardview:circlering_end_color="#1fcf6d"
                android:layout_centerInParent="true"/>

            <RelativeLayout
                android:layout_width="@dimen/x452"
                android:layout_height="@dimen/y452"
                android:layout_centerInParent="true">
                <TextView
                    android:id="@+id/tv_home_variability_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textSize="@dimen/x128"
                    android:textColor="#545454"
                    android:layout_centerInParent="true"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/tv_home_variability_value"
                    android:text="一致性"
                    android:textSize="@dimen/x36"
                    android:textColor="#545454"
                    android:layout_marginBottom="@dimen/y10"
                    android:layout_centerHorizontal="true"/>
                <TextView
                    android:id="@+id/tv_home_variability_dec"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_home_variability_value"
                    android:text="--"
                    android:textSize="@dimen/x36"
                    android:textColor="#01bf9d"
                    android:layout_marginTop="@dimen/y10"
                    android:layout_centerHorizontal="true"/>
            </RelativeLayout>
        </RelativeLayout>

        <TextView
            android:id="@+id/tv_text1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x128"
            android:text="步频"
            android:textSize="@dimen/x52"
            android:textColor="#5babe6"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:gravity="center_vertical"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x848"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:background="@drawable/bg_lineargradient_threecolor">
            <TextView
                android:id="@+id/tv_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/y28"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="步频(步/分钟)"/>

            <TextView
                android:id="@+id/tv_dec1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="时间：分钟"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/x48"/>
            <com.amsu.healthy.view.HeightCurveView
                android:id="@+id/hc_result_step"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/tv_text2"
                android:layout_marginTop="@dimen/y64"
                android:layout_marginBottom="@dimen/x56"
                android:layout_above="@+id/tv_dec1"
                HeightCurve:fillstart_color="#FFF"
                HeightCurve:fillend_color="#665babe6"
                HeightCurve:curve_line_color="#5babe6"/>

        </RelativeLayout>
        <TextView
            android:id="@+id/tv_text5"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x128"
            android:text="步幅"
            android:textSize="@dimen/x52"
            android:textColor="#bc8ccf"
            android:background="#FFF"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x848"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:background="@drawable/bg_lineargradient_threecolor">


            <TextView
                android:id="@+id/tv_text6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/y28"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="步幅(步/米)"/>

            <TextView
                android:id="@+id/tv_dec3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="时间：分钟"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/x48"/>
            <com.amsu.healthy.view.HeightCurveView
                android:id="@+id/hc_result_strideLength"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/tv_text6"
                android:layout_marginTop="@dimen/y64"
                android:layout_marginBottom="@dimen/x56"
                android:layout_above="@+id/tv_dec3"
                HeightCurve:curve_line_width="@dimen/x6"
                HeightCurve:fillstart_color="#FFF"
                HeightCurve:fillend_color="#66bc8ccf"
                HeightCurve:curve_line_color="#bc8ccf"/>

        </RelativeLayout>

        <TextView
            android:id="@+id/tv_text7"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x128"
            android:text="离地高度"
            android:textSize="@dimen/x52"
            android:textColor="#f3c500"
            android:background="#FFF"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:gravity="center_vertical"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x848"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:background="@drawable/bg_lineargradient_threecolor">


            <TextView
                android:id="@+id/tv_text8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/y28"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="高度(厘米/cm)"/>

            <TextView
                android:id="@+id/tv_dec4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="时间：分钟"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/x48"/>
            <com.amsu.healthy.view.HeightCurveView
                android:id="@+id/hc_result_stepHeightMean"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/tv_text8"
                android:layout_marginTop="@dimen/y64"
                android:layout_marginBottom="@dimen/x56"
                android:layout_above="@+id/tv_dec4"
                HeightCurve:curve_line_width="@dimen/x6"
                HeightCurve:fillstart_color="#FFF"
                HeightCurve:fillend_color="#66f3c500"
                HeightCurve:curve_line_color="#f3c500"/>

        </RelativeLayout>

        <TextView
            android:id="@+id/tv_text3"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x128"
            android:text="摆动宽度"
            android:textSize="@dimen/x52"
            android:textColor="#e87e04"
            android:background="#FFF"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:gravity="center_vertical"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x848"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:background="@drawable/bg_lineargradient_threecolor">


            <TextView
                android:id="@+id/tv_text4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/y28"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="宽度(厘米/cm)"/>

            <TextView
                android:id="@+id/tv_dec2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="时间：分钟"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/x48"/>
            <com.amsu.healthy.view.HeightCurveView
                android:id="@+id/hc_result_swingWidthMean"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/tv_text4"
                android:layout_marginTop="@dimen/y64"
                android:layout_marginBottom="@dimen/x56"
                android:layout_above="@+id/tv_dec2"
                HeightCurve:curve_line_width="@dimen/x6"
                HeightCurve:fillstart_color="#FFF"
                HeightCurve:fillend_color="#66e87e04"
                HeightCurve:curve_line_color="#e87e04"/>

        </RelativeLayout>

        <TextView
            android:id="@+id/tv_text9"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x128"
            android:text="触地时间"
            android:textSize="@dimen/x52"
            android:textColor="#ea4b35"
            android:background="#FFF"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:gravity="center_vertical"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x848"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:background="@drawable/bg_lineargradient_threecolor">


            <TextView
                android:id="@+id/tv_text10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/y28"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="时间(毫秒/ms)"/>

            <TextView
                android:id="@+id/tv_dec5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="时间：分钟"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/x48"/>
            <com.amsu.healthy.view.HeightCurveView
                android:id="@+id/hc_result_stanceDurationMean"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/tv_text10"
                android:layout_marginTop="@dimen/y64"
                android:layout_marginBottom="@dimen/x56"
                android:layout_above="@+id/tv_dec5"
                HeightCurve:curve_line_width="@dimen/x6"
                HeightCurve:fillstart_color="#FFF"
                HeightCurve:fillend_color="#66ea4b35"
                HeightCurve:curve_line_color="#ea4b35"/>

        </RelativeLayout>

        <TextView
            android:id="@+id/tv_text11"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x128"
            android:text="着地冲击"
            android:textSize="@dimen/x52"
            android:textColor="#334960"
            android:background="#FFF"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:gravity="center_vertical"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x848"
            android:paddingLeft="@dimen/x36"
            android:paddingRight="@dimen/x36"
            android:background="@drawable/bg_lineargradient_threecolor">


            <TextView
                android:id="@+id/tv_text12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/y28"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="力度(千克/kg)"/>
            <TextView
                android:id="@+id/tv_dec6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/x36"
                android:textColor="#808080"
                android:text="时间：分钟"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/x48"/>
            <com.amsu.healthy.view.HeightCurveView
                android:id="@+id/hc_result_loadingImpactMean"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/tv_text12"
                android:layout_marginTop="@dimen/y64"
                android:layout_marginBottom="@dimen/x56"
                android:layout_above="@+id/tv_dec6"
                HeightCurve:curve_line_width="@dimen/x6"
                HeightCurve:fillstart_color="#FFF"
                HeightCurve:fillend_color="#66334960"
                HeightCurve:curve_line_color="#334960"/>

        </RelativeLayout>

    </LinearLayout>



</ScrollView>
