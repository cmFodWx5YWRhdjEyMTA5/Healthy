<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_club_home_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F3F3F3"
    tools:context="com.amsu.healthy.activity.ClubHomePageActivity">

    <ListView
        android:id="@+id/lv_homepage_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll_detial_bottom"
        android:divider="@null">
    </ListView>

    <RelativeLayout
        android:id="@+id/rl_homepage_add"
        android:layout_width="@dimen/x140"
        android:layout_height="@dimen/x140"
        android:background="@drawable/bg_bt_bigcircle"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/x36"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/x56">
        <ImageView
            android:layout_width="@dimen/x52"
            android:layout_height="@dimen/x52"
            android:layout_centerInParent="true"
            android:src="@drawable/jlb_fbxdt"/>
    </RelativeLayout>

</RelativeLayout>
