<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FFFFFF"
    tools:context="com.amsu.healthy.fragment.analysis_insole.ResultDetailsFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y572"
        android:background="#f3f3f3"
        android:gravity="center">
        <TextView
            android:id="@+id/tv_detail_mileage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0:00"
            android:textColor="#1f1f1f"
            android:textSize="@dimen/x300"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/tv_detail_mileage"
            android:layout_marginLeft="@dimen/x32"
            android:layout_alignBottom="@+id/tv_detail_mileage"
            android:layout_marginBottom="@dimen/x70"
            android:text="km"
            android:textColor="#1f1f1f"
            android:textSize="@dimen/x40"/>
        <TextView
            android:id="@+id/text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_detail_mileage"
            android:text="用时 : 00:00:00"
            android:textColor="#1f1f1f"
            android:textSize="@dimen/x52"
            android:layout_centerHorizontal="true"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_r2"
        android:gravity="center_vertical"
        android:layout_marginTop="@dimen/y104"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_text3">
            <TextView
                android:id="@+id/tv_detail_stride"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="--"
                android:textColor="#1f1f1f"
                android:textSize="@dimen/x92"
                android:gravity="center"/>

            <TextView
                android:id="@+id/tv_detail_avespeed"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="--"
                android:textColor="#1f1f1f"
                android:textSize="@dimen/x92"
                android:gravity="center"/>
            <TextView
                android:id="@+id/tv_detail_freqstride"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="--"
                android:textColor="#1f1f1f"
                android:textSize="@dimen/x92"
                android:gravity="center"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y28">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="步幅：厘米"
                android:textColor="#666666"
                android:textSize="@dimen/x36"
                android:gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="均速：km/h"
                android:textColor="#666666"
                android:textSize="@dimen/x36"
                android:gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="步频"
                android:textColor="#666666"
                android:textSize="@dimen/x36"
                android:gravity="center"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_r2"
        android:gravity="center_vertical"
        android:layout_marginTop="@dimen/y148"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_text3">
            <TextView
                android:id="@+id/tv_detail_maxspeed"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="--"
                android:textColor="#1f1f1f"
                android:textSize="@dimen/x92"
                android:gravity="center"/>

            <TextView
                android:id="@+id/tv_detail_minspeed"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="--"
                android:textColor="#1f1f1f"
                android:textSize="@dimen/x92"
                android:gravity="center"/>
            <TextView
                android:id="@+id/tv_detail_kcal"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="--"
                android:textColor="#1f1f1f"
                android:textSize="@dimen/x92"
                android:gravity="center"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y28">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="最快：km/h"
                android:textColor="#666666"
                android:textSize="@dimen/x36"
                android:gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="最慢：km/h"
                android:textColor="#666666"
                android:textSize="@dimen/x36"
                android:gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="消耗：千卡"
                android:textColor="#666666"
                android:textSize="@dimen/x36"
                android:gravity="center"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rl_r2"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/y148"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ll_text3">
                <TextView
                    android:id="@+id/tv_detail_reachway"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:textColor="#1f1f1f"
                    android:textSize="@dimen/x64"
                    android:gravity="center"/>

                <TextView
                    android:id="@+id/tv_detail_outturnver"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:textColor="#1f1f1f"
                    android:textSize="@dimen/x64"
                    android:gravity="center"/>
                <TextView
                    android:id="@+id/tv_detail_singlestable"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:textColor="#1f1f1f"
                    android:textSize="@dimen/x64"
                    android:gravity="center"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/y28">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="着地方式"
                    android:textColor="#666666"
                    android:textSize="@dimen/x36"
                    android:gravity="center"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="足内外翻"
                    android:textColor="#666666"
                    android:textSize="@dimen/x36"
                    android:gravity="center"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="单腿支撑稳定性"
                    android:textColor="#666666"
                    android:textSize="@dimen/x36"
                    android:gravity="center"/>
            </LinearLayout>
    </LinearLayout>


    </LinearLayout>
</LinearLayout>
