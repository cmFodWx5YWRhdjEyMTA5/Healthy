<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FFFFFF"
    tools:context="com.amsu.healthy.activity.insole.PrepareRunningActivity">

    <include layout="@layout/page_head"
        android:id="@+id/rl_base_head">
    </include>

    <android.support.v4.view.ViewPager
        android:id="@+id/vp_prepare_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y950"></android.support.v4.view.ViewPager>


    <ImageView
        android:layout_width="@dimen/x340"
        android:layout_height="@dimen/x340"
        android:layout_marginTop="@dimen/x240"
        android:src="@drawable/kaishianniu"
        android:onClick="goStartRun"
        android:layout_gravity="center_horizontal"/>


</LinearLayout>
