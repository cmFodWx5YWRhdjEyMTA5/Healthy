<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_news_detial"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.amsu.healthy.activity.NewsDetialActivity">

    <LinearLayout
        android:id="@+id/ll_detial_bottom"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x140"
        android:orientation="horizontal"
        android:background="#FFFFFF"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true">
        <RelativeLayout
            android:id="@+id/rl_detial_back"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/x140"
            android:layout_gravity="center_vertical">
            <ImageView
                android:layout_width="@dimen/x60"
                android:layout_height="@dimen/x60"
                android:src="@drawable/news_back"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_detial_comment"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/x140"
            android:layout_gravity="center_vertical">
            <ImageView
                android:layout_width="@dimen/x60"
                android:layout_height="@dimen/x60"
                android:src="@drawable/news_pl"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_detial_collect"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/x140"
            android:layout_gravity="center_vertical">
            <ImageView
                android:layout_width="@dimen/x60"
                android:layout_height="@dimen/x60"
                android:src="@drawable/news_sc"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_detial_share"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/x140"
            android:layout_gravity="center_vertical">
            <ImageView
                android:layout_width="@dimen/x60"
                android:layout_height="@dimen/x60"
                android:src="@drawable/news_fx"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_detial_other"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="@dimen/x140"
            android:layout_gravity="center_vertical">
            <ImageView
                android:layout_width="@dimen/x60"
                android:layout_height="@dimen/x60"
                android:src="@drawable/news_gd"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
    </LinearLayout>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x1"
        android:background="#dfdede"
        android:layout_above="@+id/ll_detial_bottom"></View>

    <WebView
        android:id="@+id/wv_detial_article"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/view">
    </WebView>





</RelativeLayout>
