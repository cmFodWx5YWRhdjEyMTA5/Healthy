<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.amsu.healthy.activity.MyReportActivity">

    <include layout="@layout/page_head"
        android:id="@+id/rl_base_head">
    </include>

    <LinearLayout
        android:id="@+id/ll_analysis_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y128"
        android:orientation="horizontal"
        android:background="#212121"
        android:layout_below="@id/rl_base_head">
        <TextView
            android:id="@+id/tv_report_mouth"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="月季报告"
            android:textColor="#0c64b5"
            android:gravity="center"
            android:textSize="@dimen/x48"/>
        <TextView
            android:id="@+id/tv_report_quarter"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="季度报告"
            android:textColor="#999999"
            android:gravity="center"
            android:textSize="@dimen/x48"/>
        <TextView
            android:id="@+id/tv_report_year"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="年度报告"
            android:textColor="#999999"
            android:gravity="center"
            android:textSize="@dimen/x48"/>
    </LinearLayout>
    <View
        android:id="@+id/v_analysis_select"
        android:layout_width="@dimen/x360"
        android:layout_height="@dimen/x6"
        android:layout_below="@id/rl_base_head"
        android:layout_marginTop="@dimen/y122"
        android:background="#0c64b5"></View>

    <FrameLayout
        android:id="@+id/fragment_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/ll_analysis_title"
        >
    </FrameLayout>
</RelativeLayout>
